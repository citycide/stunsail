{
  "name": "stunsail",
  "version": "1.0.0-alpha.15",
  "description": "Super opinionated collection of utility functions.",
  "license": "MIT",
  "author": {
    "name": "Bo Lingen",
    "email": "thecitycide@gmail.com",
    "url": "github.com/citycide"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/citycide/stunsail.git"
  },
  "homepage": "https://github.com/citycide/stunsail",
  "bugs": "https://github.com/citycide/stunsail/issues",
  "engines": {
    "node": ">=4"
  },
  "main": "index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "lint": "standard \"src/**/*.js\" \"tests/**/*.js\" | snazzy",
    "docs": "trash \"docs\" && node build/build-docs.js",
    "index": "node build/create-index.js",
    "prebuild": "trash \"babel\" \"es\" \"lib\" \"*.js\" \"!.eslintrc.js\"",
    "build": "npm run index && if-not-ci npm run build:es && npm run build:main",
    "build:es": "cross-env BABEL_ENV=es babel src -d es",
    "build:main": "cross-env BABEL_ENV=production babel src -d .",
    "build:watch": "cross-env BABEL_ENV=dev babel src -d . -w",
    "pretest": "npm run build && npm run lint",
    "test": "ava",
    "test:watch": "ava -w",
    "prepublish": "npm test"
  },
  "keywords": [
    "utility",
    "tools",
    "utils",
    "collection",
    "functions"
  ],
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-babili": "0.0.12",
    "babel-preset-env": "^1.3.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.0",
    "dedent": "^0.7.0",
    "if-ci": "^2.0.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "trash-cli": "^1.4.0"
  },
  "ava": {
    "files": [
      "tests/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ]
  }
}
