{
  "name": "stunsail",
  "version": "1.0.0-rc.1",
  "description": "Super opinionated, functional utility collection.",
  "license": "MIT",
  "author": {
    "name": "Bo Lingen",
    "email": "lingenbw@gmail.com",
    "url": "https://github.com/citycide"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/citycide/stunsail.git"
  },
  "homepage": "https://github.com/citycide/stunsail",
  "bugs": "https://github.com/citycide/stunsail/issues",
  "engines": {
    "node": ">=6"
  },
  "main": "index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "*.js",
    "babel",
    "es",
    "lib"
  ],
  "scripts": {
    "lint": "eslint src tests",
    "docs": "cpy \"*.md\" docs",
    "index": "node script/create-index.js",
    "prebuild": "trash \"babel\" \"prod\" \"es\" \"lib\" \"*.js\"",
    "build": "npm run index && npm run build:prod && if-not-ci npm run build:es",
    "build:prod": "cross-env NODE_ENV=production babel src -d prod",
    "build:dev": "babel src -d prod",
    "build:es": "cross-env NODE_ENV=esm babel src -d prod/es",
    "minify": "squash prod -o .",
    "pretest": "npm run build:dev",
    "test": "ava",
    "test:watch": "ava -w",
    "prepublishOnly": "npm run lint && npm run build && ava && npm run minify"
  },
  "keywords": [
    "utility",
    "tools",
    "utils",
    "collection",
    "functions"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@citycide/changelog": "^1.0.0",
    "ava": "^1.1.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-macros": "^2.4.5",
    "butternut": "^0.4.6",
    "cpy-cli": "^2.0.0",
    "cross-env": "^5.2.0",
    "dedent": "^0.7.0",
    "eslint": "^5.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "if-ci": "^3.0.0",
    "param.macro": "^2.1.0",
    "trash-cli": "^1.4.0"
  }
}
