{
  "name": "stunsail",
  "version": "1.0.0-rc.1",
  "description": "Super opinionated, functional utility collection.",
  "license": "MIT",
  "author": {
    "name": "Bo Lingen",
    "email": "lingenbw@gmail.com",
    "url": "https://github.com/citycide"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/citycide/stunsail.git"
  },
  "homepage": "https://github.com/citycide/stunsail",
  "bugs": "https://github.com/citycide/stunsail/issues",
  "engines": {
    "node": ">=6"
  },
  "main": "index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "*.js",
    "babel",
    "es",
    "lib"
  ],
  "scripts": {
    "lint": "standard \"src/**/*.js\" \"tests/**/*.js\" | snazzy",
    "docs": "cpy \"*.md\" docs",
    "index": "node script/create-index.js",
    "prebuild": "trash \"babel\" \"prod\" \"es\" \"lib\" \"*.js\"",
    "build": "npm run index && npm run build:prod && if-not-ci npm run build:es",
    "build:prod": "cross-env NODE_ENV=production babel src -d prod",
    "build:dev": "babel src -d prod",
    "build:es": "cross-env NODE_ENV=esm babel src -d prod/es",
    "minify": "squash prod -o .",
    "pretest": "npm run build:dev",
    "test": "ava",
    "test:watch": "ava -w",
    "prepublishOnly": "npm run lint && npm run build && ava && npm run minify"
  },
  "keywords": [
    "utility",
    "tools",
    "utils",
    "collection",
    "functions"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.38",
    "@babel/core": "7.0.0-beta.38",
    "@babel/preset-env": "7.0.0-beta.38",
    "@babel/preset-stage-1": "7.0.0-beta.38",
    "@citycide/changelog": "^1.0.0",
    "ava": "^1.0.0-beta.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "butternut": "^0.4.6",
    "cpy-cli": "^1.0.1",
    "cross-env": "^5.1.1",
    "dedent": "^0.7.0",
    "if-ci": "^2.0.2",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "trash-cli": "^1.4.0"
  },
  "ava": {
    "files": [
      "tests/**/*.js"
    ],
    "sources": [
      "src/**/*.js"
    ]
  }
}
